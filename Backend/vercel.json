{
  "version": 2,
  "builds": [
    {
      "src": "index-minimal.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index-minimal.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index-minimal.js"
    }
  ],
  "functions": {
    "index-minimal.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
